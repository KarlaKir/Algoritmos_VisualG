Algoritmo "Conversor Real x Dolar"

//Bootcamp Programador(a) de Software Iniciante//

Var
   ValorEmReal, Cotacao, Resultado: Real
   
Inicio
      Escreva ("Informe o valor em Real: ")
      Leia (ValorEmReal)
      
      Escreva ("Informe a cotação: ")
      Leia (Cotacao)
      
      Resultado <- ValorEmReal / Cotacao
      Escreva ("O valor em dolar é", Resultado)

Fimalgoritmo