Algoritmo "CalculadoraIdade"

//Bootcamp Programador(a) de Software Iniciante//

Var
   AnoAtual, AnoNascimento, Resultado: Inteiro


Inicio
   Escreva ("Informe o ano atual: ")
   Leia (AnoAtual)
   
   Escreva ("Informe o ano de seu nascimento: ")
   Leia (AnoNascimento)
   
   Resultado <- AnoNascimento - AnoAtual
   Escreva (" Sua idade é: ", Resultado)
   
Fimalgoritmo